<%- include('header') %>
    <!-- END HEADER DESKTOP-->

    <!-- MAIN CONTENT-->
    <div class="main-content">
        <div class="section__content section__content--p30">
            <div class="container-fluid">
                <form class="d-flex m-5">
                    <input class="form-control bg-secondary border-2" style="width: 500px;" type="search" placeholder="search by name, email, gender, and city" name="search">
                    <button class="ms-3 bg-secondary border-0 text-white">Search</button>
                </form>
                <div class="row">
                    <div class="col-lg-12">
                        <div class="table-responsive table--no-card m-b-30">
                            <table class="table table-borderless table-striped table-earning">
                                <thead>
                                    <tr>
                                        <th>No</th>
                                        <th>Category</th>
                                        <th>Subcategory</th>
                                        <th>Description</th>
                                        <th>Date</th>
                                        <th>Image</th>
                                        <th>Status</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% var i=0 %>
                                        <% data.map((v,i)=> { %>
                                            <tr>
                                                <td>
                                                    <%= ++i %>
                                                </td>
                                                <td>
                                                    <%= v.categoryId.category %>
                                                </td>
                                                <td>
                                                    <%= v.title %>
                                                </td>
                                                <td>
                                                    <%= v.description %>
                                                </td>
                                                <td>
                                                    <%= v.create_date %>
                                                </td>
                                                <td>
                                                    <img src="<%= v.categoryimage %>" alt="" width="100" height="100">
                                                </td>
                                                <td>
                                                    <% if(data.status){ %> 
                                                        <a href="/admin/category/deactive/<%= data.id %>" class="btn btn-success">Active</a>
                                                    <% }else{ %> 
                                                        <a href="/admin/category/active/<%= data.id %>" class="btn btn-danger">Deactive</a>
                                                    <% } %>
                                                </td>
                                                <td>
                                                    <a href="/admin/testimonial/testimonialdelete/<%= v.id %>">
                                                        <i class="fa fa-trash"></i>
                                                    </a>||
                                                    <a href="/admin/testimonial/testimonialupdate/<%= v.id %>">
                                                        <i class="fa fa-edit"></i>
                                                    </a>
                                                </td>
                                            </tr>
                                            <% }) %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <%- include('footer') %>