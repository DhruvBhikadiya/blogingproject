<%- include('header') %>
    <!-- END HEADER DESKTOP-->

    <!-- MAIN CONTENT-->
    <div class="main-content">
        <div class="section__content section__content--p30">
            <div class="container-fluid">
                <form class="d-flex m-5">
                    <input class="form-control bg-secondary border-2" style="width: 500px;" type="search" placeholder="search by name, email, gender, and city" name="search">
                    <button class="ms-3 bg-secondary border-0 text-white">Search</button>
                </form>
                <div class="row">
                    <div class="col-lg-12">
                        <div class="table-responsive table--no-card m-b-30">
                            <table class="table table-borderless table-striped table-earning">
                                <thead>
                                    <tr>
                                        <th>No</th>
                                        <th>Title</th>
                                        <th>Description</th>
                                        <th>Photos</th>
                                        <th>Actions</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% var i=0 %>
                                        <% data.map((v,i)=> { %>
                                            <tr>
                                                <td>
                                                    <%= ++i %>
                                                </td>
                                                <td>
                                                    <%= v.title %>
                                                </td>
                                                <td>
                                                    <%= v.description %>
                                                </td>
                                                <td>
                                                    <img src="<%= v.photosimage %>" alt="image" title="image" width="200" height="150">
                                                </td>
                                                <td>
                                                    <a href="/admin/photos/photosdelete/<%= v.id %>">
                                                        <i class="fa fa-trash"></i>
                                                    </a>||
                                                    <a href="/admin/photos/photosupdate/<%= v.id %>">
                                                        <i class="fa fa-edit"></i>
                                                    </a>
                                                </td>
                                            </tr>
                                            <% }) %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <%- include('footer') %>